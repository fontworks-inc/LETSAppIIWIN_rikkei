    <Compile Include="Behaviors\PasswordBindingBehavior.cs" />
    <Compile Include="Components\ApplicationIconComponent.cs" />
    <Compile Include="Components\ApplicationIconMode.cs" />
    <Compile Include="Components\ComponentManager.cs" />
    <Compile Include="Components\ComponentManager.Designer.cs" />
    <Compile Include="Components\LParamType.cs" />
