    <Compile Include="Exceptions\GetResetPasswordPageUrlException.cs" />
    <Compile Include="Exceptions\GetUserRegistrationPageUrlException.cs" />
    <Compile Include="Interfaces\IComponentManagerWrapper.cs" />
    <Compile Include="Interfaces\ILoginWindowWrapper.cs" />
    <Compile Include="Resources\Resource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>
    <Compile Include="Shell.cs" />
    <Compile Include="ViewModels\DeviceSettingsViewModel.cs" />
    <Compile Include="ViewModels\DeviceViewModel.cs" />
    <Compile Include="ViewModels\ForceLogoutNotificationViewModel.cs" />
    <Compile Include="ViewModels\LoginCompletedViewModel.cs" />
