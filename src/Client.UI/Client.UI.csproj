    <Compile Include="Wrappers\ComponentManagerWrapper.cs" />
    <Compile Include="Wrappers\LoginWindowWrapper.cs" />
    <Compile Include="Wrappers\ResourceWrapper.cs" />
    <Compile Include="Wrappers\ToastNotificationWrapper.cs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="appsettings.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
    <Content Include="LETS.ico">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <None Include="nlog.config">
